{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      }
    ],
    "build": {
        "env": {
            "PRISMA_CLIENT_ENGINE_TYPE": "binary"
        }
    }
  }
  